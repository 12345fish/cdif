{
  "urn:cdif-net:serviceID:ONVIFMediaService": {
    "actionList": {
      "GetStreamUri": {
        "argumentList": {
          "streamType": {
            "direction": "in",
            "relatedStateVariable": "streamType"
          },
          "transport": {
            "direction": "in",
            "relatedStateVariable": "transport"
          },
          "streamUri": {
            "direction": "out",
            "retval": true,
            "relatedStateVariable": "streamUri"
          }
        }
      }
    },
    "serviceStateTable": {
      "streamType": {
        "sendEvents": "no",
        "dataType": "string",
        "allowedValueList": [
          "RTP-Unicast",
          "RTP-Multicast",
          "MJPEG-Unicast"
        ],
        "defaultValue": "RTP-Unicast"
      },
      "transport": {
        "sendEvents": "no",
        "dataType": "string",
        "allowedValueList": [
          "UDP",
          "TCP",
          "RTSP",
          "HTTP",
          "WebSocket"
        ],
        "defaultValue": "WebSocket"
      },
      "streamUri": {
        "sendEvents": "yes",
        "dataType": "url"
      }
    }
  }
}